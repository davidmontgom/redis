[program:sentinal_python]
command = /usr/bin/python /var/sentinal-master-zk.py
autostart=true
autorestart=true

stopsignal=KILL
stopasgroup = true
killasgroup = true

stdout_logfile=/tmp/sentinal_python.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/sentinal_python.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1

[group:sentinal_python_server]
programs=sentinal_python

