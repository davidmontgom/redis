[program:sentinal_zookeeper]
command = /usr/bin/python /var/sentinal-zookeeper.py
autostart=true
autorestart=true

stopsignal=KILL
stopasgroup = true
killasgroup = true

stdout_logfile=/tmp/sentinal_zookeeper.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/sentinal_zookeeper.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1

[group:sentinal_zookeeper_server]
programs=sentinal_zookeeper

